{
  "managedApiConnections": {
    "servicebus": {
      "api": {
        "id": "/subscriptions/@{appsetting('WORKFLOWS_SUBSCRIPTION_ID')}/providers/Microsoft.Web/locations/@{appsetting('WORKFLOWS_LOCATION_NAME')}/managedApis/servicebus"
      },
      "connection": {
        "id": "/subscriptions/@{appsetting('WORKFLOWS_SUBSCRIPTION_ID')}/resourceGroups/@{appsetting('WORKFLOWS_RESOURCE_GROUP_NAME')}/providers/Microsoft.Web/connections/servicebus"
      },
      "connectionRuntimeUrl": "@parameters('servicebus-ConnectionRuntimeUrl')",
      "authentication": "@parameters('servicebus-Authentication')"
    },
    "commondataservice": {
      "api": {
        "id": "/subscriptions/@{appsetting('WORKFLOWS_SUBSCRIPTION_ID')}/providers/Microsoft.Web/locations/@{appsetting('WORKFLOWS_LOCATION_NAME')}/managedApis/commondataservice"
      },
      "connection": {
        "id": "/subscriptions/@{appsetting('WORKFLOWS_SUBSCRIPTION_ID')}/resourceGroups/@{appsetting('WORKFLOWS_RESOURCE_GROUP_NAME')}/providers/Microsoft.Web/connections/commondataservice"
      },
      "connectionRuntimeUrl": "@parameters('commondataservice-ConnectionRuntimeUrl')",
      "authentication": "@parameters('commondataservice-Authentication')"
    },
  
    "sql-1": {
      "api": {
        "id": "/subscriptions/@{appsetting('WORKFLOWS_SUBSCRIPTION_ID')}/providers/Microsoft.Web/locations/@{appsetting('WORKFLOWS_LOCATION_NAME')}/managedApis/sql"
      },
      "connection": {
        "id": "/subscriptions/@{appsetting('WORKFLOWS_SUBSCRIPTION_ID')}/resourceGroups/@{appsetting('WORKFLOWS_RESOURCE_GROUP_NAME')}/providers/Microsoft.Web/connections/sql-1"
      },
      "connectionRuntimeUrl": "@parameters('sql-1-ConnectionRuntimeUrl')",
      "authentication": "@parameters('sql-1-Authentication')",
      "connectionProperties": {
        "authentication": {
          "type": "ManagedServiceIdentity",
          "audience": "https://database.windows.net/"
        }
      }
    }
  },
  "serviceProviderConnections": {
    "azuretables": {
      "parameterValues": {
        "connectionString": "@appsetting('azureTables_connectionString')"
      },
      "parameterSetName": "connectionString",
      "serviceProvider": {
        "id": "/serviceProviders/azureTables"
      },
      "displayName": "azuretable"
    }
  }
}
